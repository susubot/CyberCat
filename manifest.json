{
	"manifest_version": 3,
	"name": "CyberCat",
	"description": "ᓚ₍ ^. .^₎ Check your Password Strength (and more coming soon)!",
	"version": "1.0",
	"icons": {
		"16": "icons/catLogo16.png",
		"32": "icons/catLogo32.png",
		"48": "icons/catLogo48.png",
		"128": "icons/catLogo128.png"
	},
	"action": {
		"default_popup": "popup1.html",
		"default_icon": {
			"16": "icons/catLogo16.png",
			"32": "icons/catLogo32.png",
			"48": "icons/catLogo48.png",
			"128": "icons/catLogo128.png"
		}
	},
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
